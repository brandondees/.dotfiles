# Aliases

alias home='cd ~ && ls -ABCF --color=auto'

# Git Aliases
alias gfc='git fetch upstream && git checkout upstream/master'
alias gfr='git fetch upstream && git rebase -i upstream/master'
alias status='git status'
alias add='git add'
alias commitm='git commit -m'
alias push='git push'
alias pull='git pull'
